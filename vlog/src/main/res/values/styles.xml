<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>
    <style name="custom_theme" parent="Theme.AppCompat.Dialog">
        <item name="android:textColor">#FF00DFFF</item>
    </style>
    <style name="splash" parent="Theme.AppCompat.NoActionBar"></style>
    <style name="GPlusProgressBar" parent="SmoothProgressBar">
